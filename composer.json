{
    "name": "zohomail/laravel-zeptomail",
    "description": "Custom Mail Driver Plugin for Laravel",
    "type": "library",
    "keywords": [
        "ZeptoMail",
        "ZOHO",
        "email",
        "transactional",
        "laravel-driver"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ZohoMail\\LaravelZeptoMail\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Zoho Mail",
            "email": "zmintegration@zohomail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "illuminate/support": "^9.0 || ^10.0  || ^11.0 || ^12.0",
        "nyholm/psr7": "^1.5",
        "php-http/guzzle7-adapter": "^1.0",
        "symfony/mailer": "^6.0 || ^7.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ZohoMail\\LaravelZeptoMail\\LaravelDriverServiceProvider"
            ]
        }
    }
}
